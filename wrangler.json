{
  "name": "ohlaw-www-2025",
  "compatibility_date": "2024-11-01",
  "compatibility_flags": ["nodejs_compat"],
  "build": {
    "command": "pnpm build"
  },
  "env": {
    "production": {
      "name": "ohlaw-www-2025-production",
      "vars": {
        "NODE_ENV": "production",
        "NUXT_ENV": "production"
      },
      "d1_databases": [
        {
          "binding": "STATUTE_DB",
          "database_name": "ohlaw-statutes-prod",
          "database_id": "YOUR_PROD_D1_DATABASE_ID"
        }
      ],
      "r2_buckets": [
        {
          "binding": "STATUTE_BUCKET",
          "bucket_name": "ohlaw-statutes-assets-prod"
        }
      ],
      "kv_namespaces": [
        {
          "binding": "STATUTES_CACHE",
          "id": "YOUR_PROD_KV_NAMESPACE_ID"
        }
      ],
      "analytics_engine_datasets": [
        {
          "binding": "ANALYTICS"
        }
      ],
      "vectorize": [
        {
          "binding": "STATUTE_VECTORIZE",
          "index_name": "statute-embeddings-prod"
        }
      ]
    },
    "preview": {
      "name": "ohlaw-www-2025-preview",
      "vars": {
        "NODE_ENV": "preview",
        "NUXT_ENV": "preview"
      },
      "d1_databases": [
        {
          "binding": "STATUTE_DB",
          "database_name": "ohlaw-statutes-preview",
          "database_id": "YOUR_PREVIEW_D1_DATABASE_ID"
        }
      ],
      "r2_buckets": [
        {
          "binding": "STATUTE_BUCKET",
          "bucket_name": "ohlaw-statutes-assets-preview"
        }
      ],
      "kv_namespaces": [
        {
          "binding": "STATUTES_CACHE",
          "id": "YOUR_PREVIEW_KV_NAMESPACE_ID"
        }
      ],
      "analytics_engine_datasets": [
        {
          "binding": "ANALYTICS"
        }
      ],
      "vectorize": [
        {
          "binding": "STATUTE_VECTORIZE",
          "index_name": "statute-embeddings-preview"
        }
      ]
    },
    "development": {
      "name": "ohlaw-www-2025-dev",
      "vars": {
        "NODE_ENV": "development",
        "NUXT_ENV": "development"
      },
      "d1_databases": [
        {
          "binding": "STATUTE_DB",
          "database_name": "ohlaw-statutes-dev",
          "database_id": "YOUR_DEV_D1_DATABASE_ID"
        }
      ],
      "r2_buckets": [
        {
          "binding": "STATUTE_BUCKET",
          "bucket_name": "ohlaw-statutes-assets-dev"
        }
      ],
      "kv_namespaces": [
        {
          "binding": "STATUTES_CACHE",
          "id": "YOUR_DEV_KV_NAMESPACE_ID"
        }
      ],
      "analytics_engine_datasets": [
        {
          "binding": "ANALYTICS"
        }
      ],
      "vectorize": [
        {
          "binding": "STATUTE_VECTORIZE",
          "index_name": "statute-embeddings-dev"
        }
      ]
    }
  }
}